<project>
    <build>
        <plugins>
            <plugin>
                <groupId>app.myoss.cloud.maven.plugins</groupId>
                <artifactId>archetypes-maven-plugin</artifactId>
                <configuration>
                    <directory>
                        ${basedir}/target/test-classes/unit/spring-boot-multi-module-project-test/buildDirectory
                    </directory>
                    <groupId>app.myoss.cloud</groupId>
                    <artifactId>multi-module-demo3</artifactId>
                    <version>1.0.0.RELEASE</version>
                    <rootPackageName>app.myoss.cloud.demo3</rootPackageName>
                    <author>Jerry.Chen</author>
                    <copyright>
